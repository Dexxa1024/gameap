{
    "name": "knik/gameap",
    "description": "Game Admin Panel",
    "keywords": ["games", "panel"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Nikita Kuznetsov",
            "email": "nikita.hldm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "~4.0",
        "fzaninotto/faker": "~1.4",
        "gameap/gameap-file-manager": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "infyomlabs/generator-builder": "dev-master",
        "joshbrw/laravel-module-installer": "^0.1.4",
        "knik/flysystem-gameap": "dev-master",
        "knik/gameap-daemon-client": "^0.3",
        "knik/gameq": "^3.0",
        "knik/x509": "^0.6.2",
        "laravel/framework": "^5.8.0",
        "laravel/passport": "^7.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.8.0",
        "league/flysystem": "^1.0",
        "mavinoo/laravel-batch": "dev-master",
        "moontoast/math": "^1.1",
        "nwidart/laravel-modules": "^5.0",
        "phpseclib/phpseclib": "~2.0",
        "silber/bouncer": "v1.0.0-rc.6",
        "spatie/laravel-query-builder": "^1.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "~2.0",
        "infyomlabs/adminlte-templates": "5.8.x-dev",
        "infyomlabs/laravel-generator": "5.8.x-dev",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Gameap\\": "app/",
            "App\\": "app/",
            "GameapModules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "module-dir": "modules"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
